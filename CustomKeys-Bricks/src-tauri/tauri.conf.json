{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "CustomKeys-Bricks",
  "version": "0.1.0",
  "identifier": "com.customkeys.bricks",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5137",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "CustomKeys-Bricks",
        "width": 360,
        "height": 650,
        "resizable": false,
        "maximizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}